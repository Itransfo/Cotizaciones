@model IEnumerable<Cotizaciones.Models.ApplicationUser>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                <span onclick="return confirm('Are you sure to delete?')">@Html.ActionLink("Delete", "Delete", new { userName = item.UserName })</span> 
               
            </td>
        </tr>
    }

</table>
